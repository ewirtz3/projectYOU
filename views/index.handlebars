<div id="homepage">
        <div class="container">
            <div class="row">
                <div class="col-8 offset-2">
                    <h1 class="text-center pageTitle mt-4">Welcome to Project You!</h1>
                </div>
            </div>
            <hr style="background: black; border: none; height: 1px;">
            <div class="row">
                <div class="col-12">
                  <div class="jumbotron aboutApp">
                     <p class="lead text-center" style="color: coral; font-weight: bold;"> With using the Project You application you will be able to turn one day into day one</p> 
                  </div>  
                </div>

            </div>
            <div class="row">
                <div class="col-md-8">
                    <h2>Log in</h2>
                    <form class="login">
                        <div class="form-group">
                          <label for="LoginEmail1">Email address</label>
                          <input type="email" class="form-control" id="email-login" placeholder="Email">
                        </div>
                        <div class="form-group">
                          <label for="LoginPassword1">Password</label>                                           
                          <input type="password" class="form-control" id="password-login" placeholder="Password">
                        </div>
                         <div style="display: none" id="alert" class="alert alert-danger" role="alert">
                          <span class="glyphicon glyphicon-exclamation-sign" aria-hidden="true"></span>
                          <span class="sr-only">Error:</span> <span class="msg"></span>
                        </div>
                        <button type="submit" class="btn btn-default logInBtn">Log In</button>
                      </form>  
                       
                </div>
                  
                <div class="col-md-4">
                  <h2>Sign Up</h2>
                  <form class="signup">
                    <div class="form-group">
                      <label for="SignupEmail">Email address</label>
                      <input type="email" class="form-control" id="email-signup" placeholder="Email">
                    </div>
                    <div class="form-group">
                      <label for="SignupPassword">Password</label>
                      <input type="password" class="form-control" id="password-signup" placeholder="Password">
                    </div>
                    <div class="form-group">
                      <label for="SignupFirstName">First Name</label>
                      <input type="text" class="form-control" id="first-name-signup" placeholder="First Name">
                    </div>
                    <div class="form-group">
                      <label for="SignupLastName">Last Name</label>
                      <input type="text" class="form-control" id="last-name-signup" placeholder="Last Name">
                    </div>
                     <div style="display: none" id="alert" class="alert alert-danger" role="alert">
                      <span class="glyphicon glyphicon-exclamation-sign" aria-hidden="true"></span>
                      <span class="sr-only">Error:</span> <span class="msg"></span>
                    </div> 
                    <button type="submit" class="btn btn-default signUpBtn">Sign Up</button>
                  </form>
                  <br />
                </div>
              </div>
            </div>
        </div>
    </div>

<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="/public/js/practice-index.js"></script>
